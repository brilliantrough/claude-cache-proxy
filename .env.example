# Claude Proxy Configuration

# 目标 Anthropic API URL
ANTHROPIC_API_URL=https://api.anthropic.com

# Anthropic API Key
ANTHROPIC_API_KEY=your_api_key_here

# 代理服务器配置
PROXY_HOST=0.0.0.0
PROXY_PORT=9999

# 缓存控制配置
CACHE_CONTROL_TTL=1h  # 默认缓存时间，支持: 5m (5分钟) 或 1h (1小时)

# 日志级别
LOG_LEVEL=INFO


# OpenAI API URL (OpenRouter)
OPENROUTER_API_URL=https://openrouter.ai/api

# OpenAI API Key (OpenRouter)
OPENROUTER_API_KEY=your_openrouter_key_here

# OpenAI 代理服务器配置
OPENAI_PROXY_HOST=0.0.0.0
OPENAI_PROXY_PORT=9998
